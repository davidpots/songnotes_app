          </ul>
        </div> <!-- end: .song-nav -->

        <%# ----- Song Lyrics ----- %>    
        <section id="lyrics">
          <%= render :partial => '_song_lyrics_2' %>
        </section>
        
        <%# ----- Notes and Tabs ----- %>
        <section id="notes_tabs">
          <h2 class="ribbon">Notes &amp; Tabs</h2>
          <%= render :partial => '_song_tabs_1' %>
        </section>
        
        <%# ----- Videos ----- %>
        <section id="videos">
          <h2 class="ribbon">Listen &amp; Play</h2>
          <%= render :partial => '_song_listen_play' %>
        </section>
        
        <%# ----- Chord Progressions ----- %>
        <section id="chord_progressions">
          <h2 class="ribbon">Chord Progressions</h2>
          <%= render :partial => '_song_chord_prog' %>
        </section>
        
        <%# ----- About Song ----- %>
        <section id="song_info">
          <h2 class="ribbon">About This Song</h2>
          <%= render :partial => '_song_about' %>
        </section>

      </div> <!-- end: .unit .song_show -->
    </div> <!-- end: .grid -->
    
